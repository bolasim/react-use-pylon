{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "outputs": ["dist/**"],
        "dependsOn": ["^build"]
      },
      "test": {
        "outputs": ["coverage/**"],
        "dependsOn": []
      },
      "e2e": {
        "cache": false,
        "persistent": true
      },
      "e2e:open": {
        "cache": false,
        "persistent": true
      },
      "lint": {},
      "lint:fix": {},
      "dev": {
        "cache": false,
        "persistent": true
      },
      "clean": {
        "cache": false
      }
    }
  }